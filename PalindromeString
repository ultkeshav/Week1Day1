// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;

int main() {
    
    string s;
    cout<<"Enter your string"<<endl;
    cin>>s;
    
    int i=0;
    int j = s.length()-1;
    bool isPalindrome = true;
    
    while(i<j){
        
        if(s[i] != s[j]){
            
            isPalindrome = false;
            break;
        }
        i++;
        j--;
    }
    
    if(isPalindrome){
        cout<<s<<" is a Palindrome String"<<endl;
    }else
        cout<<"not a palindrome string"<<endl;

    return 0;
}
