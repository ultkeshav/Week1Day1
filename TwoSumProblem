// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;

int main() {
    
    int target = 45;
    int arr[] = {1,2,3,43,56};
    int n = sizeof(arr)/sizeof(arr[0]);
    
    bool found = false;
     
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            
            if(arr[i] + arr[j] == target){
                
               cout<<"Indices are "<<i<<" and "<<j<<endl;
               found = true;
               break;
            }
        }
        if(found) break;
        
    }
    
    if(!found){
        
        cout<<"No Pair Found"<<endl;
    }

    return 0;
}
